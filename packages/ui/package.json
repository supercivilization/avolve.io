{
  "name": "@my/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./src",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "generate-theme": "tamagui generate-themes ./src/themes/theme.ts ./src/themes/theme-generated.ts",
    "check:type": "tsc --outDir /tmp",
    "check:type:watch": "yarn check:type -w",
    "check-circular-deps": "../../scripts/check-circular-deps.sh ./src/index.tsx"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@hookform/resolvers": "^5.2.1",
    "@ngneat/falso": "^8.0.2",
    "@tamagui/animations-css": "^1.141.5",
    "@tamagui/animations-moti": "^1.141.5",
    "@tamagui/animations-react-native": "^1.141.5",
    "@tamagui/cli": "^1.141.5",
    "@tamagui/config": "^1.141.5",
    "@tamagui/create-theme": "^1.141.5",
    "@tamagui/font-inter": "^1.141.5",
    "@tamagui/helpers-icon": "^1.141.5",
    "@tamagui/react-native-media-driver": "^1.141.5",
    "@tamagui/shorthands": "^1.141.5",
    "@tamagui/theme-builder": "^1.141.5",
    "@tamagui/themes": "^1.141.5",
    "@tamagui/toast": "^1.141.5",
    "@tanstack/react-table": "^8.21.3",
    "@ts-react/form": "1.6.4",
    "@vxrn/color-scheme": "^1.2.16",
    "awesome-phonenumber": "^7.6.0",
    "hast-util-to-html": "^9.0.5",
    "moti": "^0.30.0",
    "parse-numeric-range": "^1.3.0",
    "react-hook-form": "^7.62.0",
    "refractor": "^5.0.0",
    "rehype-parse": "^9.0.1",
    "swr": "^2.3.6",
    "tamagui": "^1.141.5",
    "unified": "^11.0.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tamagui/build": "^1.141.5",
    "eslint": "^9.39.1",
    "eslint-config-custom": "*",
    "typescript": "~5.9.3"
  }
}
