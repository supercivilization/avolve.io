{
  "name": "@unified/lib",
  "version": "0.1.0",
  "private": true,
  "description": "Shared utilities and AI tools for unified platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./ai": {
      "types": "./dist/ai.d.ts",
      "import": "./dist/ai.mjs",
      "require": "./dist/ai.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "ai:generate": "mcp-to-ai-sdk generate --output=./src/tools/"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.16",
    "@ai-sdk/google": "^2.0.14", 
    "@ai-sdk/openai": "^2.0.30",
    "@supabase/supabase-js": "^2.57.4",
    "ai": "^5.0.47",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "eslint": "^9",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}